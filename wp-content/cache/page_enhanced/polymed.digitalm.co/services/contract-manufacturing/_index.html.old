      <li class="active">
        <a href="/services/contract-manufacturing/">
          <div class="nav-icon man-icon"></div>
          <p>Contract Manufacturing Services</p>
          <div class="pointer"></div>
        </a>
      </li>
      <li>
        <a href="/services/analytical-services/">
          <div class="nav-icon anty-icon"></div>
          <p>Analytical Services</p>
        </a>
      </li>
      <li>
        <a href="/services/quality/">
